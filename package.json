{
  "name": "node-19-typescript",
  "type": "module",
  "version": "0.0.1",
  "description": "Node v19 with TypeScript using ECMAScript modules and jests test in CommonJS.",
  "keywords": [
    "node",
    "v19.0.0",
    "esnext",
    "module",
    "typescript",
    "watch",
    "jest",
    "commonjs"
  ],
  "bugs": "https://github.com/zakarynichols/node-19-typescript/issues",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js -c=jest.config.js --watch ./build",
    "watch-dev": "rm -rf build && tsc --watch",
    "watch-build": "node --watch build/index.js",
    "build-dist": "rm -rf dist && tsc --project tsconfig.prod.json"
  },
  "author": "Zakary Nichols",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "jest": "^29.2.1",
    "typescript": "^4.8.4"
  }
}
